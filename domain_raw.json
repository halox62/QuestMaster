{
  "domain_name": "brava-island-adventure",
  "requirements": ["strips", "typing"],
  "types": ["player", "location", "item", "trap", "puzzle", "ally"],
  "predicates": [
    "(player-at ?p - player ?l - location)",
    "(has ?p - player ?i - item)",
    "(ally-present ?a - ally ?l - location)",
    "(wounded ?p - player)",
    "(medallion-found)",
    "(native-trust-earned)",
    "(puzzle-solved)",
    "(treasure-claimed)",
    "(temple-accessible)",
    "(trap-active ?t - trap ?l - location)",
    "(trap-triggered ?t - trap ?l - location)",
    "(at-jungle-edge)",
    "(at-shore)",
    "(at-village)",
    "(at-collapsed-cave)",
    "(lost-in-jungle)",
    "(fatally-injured ?p - player)",
    "(trapped ?p - player)"
  ],
  "actions": [
    {
      "name": "venture-into-dense-jungle",
      "parameters": ["?p - player", "?shore - location", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?shore)", "(at-shore)"],
      "effects": [
        "(not (player-at ?p ?shore))",
        "(player-at ?p ?jungle-edge)",
        "(not (at-shore))",
        "(at-jungle-edge)"
      ]
    },
    {
      "name": "scout-along-shoreline",
      "parameters": ["?p - player", "?shore - location", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?shore)", "(at-shore)", "(not (wounded ?p))"],
      "effects": [
        "(not (player-at ?p ?shore))",
        "(player-at ?p ?jungle-edge)",
        "(not (at-shore))",
        "(at-jungle-edge)",
        "(not (wounded ?p))"
      ]
    },
    {
      "name": "rest-and-prepare-supplies",
      "parameters": ["?p - player", "?shore - location"],
      "preconditions": ["(player-at ?p ?shore)", "(at-shore)", "(not (wounded ?p))"],
      "effects": [
        "(player-at ?p ?shore)",
        "(at-shore)",
        "(not (wounded ?p))"
      ]
    },
    {
      "name": "call-out-for-natives",
      "parameters": ["?p - player", "?shore - location", "?native - ally"],
      "preconditions": ["(player-at ?p ?shore)", "(at-shore)", "(not (ally-present ?native ?shore))"],
      "effects": [
        "(ally-present ?native ?shore)"
      ]
    },
    {
      "name": "proceed-cautiously-to-collapsed-cave",
      "parameters": ["?p - player", "?jungle-edge - location", "?cave - location"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)"],
      "effects": [
        "(not (player-at ?p ?jungle-edge))",
        "(player-at ?p ?cave)",
        "(not (at-jungle-edge))",
        "(at-collapsed-cave)"
      ]
    },
    {
      "name": "explore-jungle-outskirts-for-allies",
      "parameters": ["?p - player", "?jungle-edge - location", "?native - ally"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)"],
      "effects": [
        "(ally-present ?native ?jungle-edge)"
      ]
    },
    {
      "name": "set-simple-traps",
      "parameters": ["?p - player", "?jungle-edge - location", "?trap - trap"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)", "(not (wounded ?p))"],
      "effects": [
        "(trap-active ?trap ?jungle-edge)",
        "(not (wounded ?p))"
      ]
    },
    {
      "name": "head-into-jungle-to-collapsed-cave",
      "parameters": ["?p - player", "?jungle-edge - location", "?cave - location"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)"],
      "effects": [
        "(not (player-at ?p ?jungle-edge))",
        "(player-at ?p ?cave)",
        "(not (at-jungle-edge))",
        "(at-collapsed-cave)"
      ]
    },
    {
      "name": "follow-smoke-to-village",
      "parameters": ["?p - player", "?jungle-edge - location", "?village - location"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)"],
      "effects": [
        "(not (player-at ?p ?jungle-edge))",
        "(player-at ?p ?village)",
        "(not (at-jungle-edge))",
        "(at-village)"
      ]
    },
    {
      "name": "set-up-temporary-camp",
      "parameters": ["?p - player", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)", "(not (wounded ?p))"],
      "effects": [
        "(player-at ?p ?jungle-edge)",
        "(at-jungle-edge)",
        "(not (wounded ?p))"
      ]
    },
    {
      "name": "search-carefully-for-medallion",
      "parameters": ["?p - player", "?cave - location", "?medallion - item"],
      "preconditions": ["(player-at ?p ?cave)", "(at-collapsed-cave)", "(not (wounded ?p))"],
      "effects": [
        "(medallion-found)",
        "(has ?p ?medallion)"
      ]
    },
    {
      "name": "rush-through-cave-and-get-wounded",
      "parameters": ["?p - player", "?cave - location", "?trap - trap"],
      "preconditions": ["(player-at ?p ?cave)", "(at-collapsed-cave)", "(not (wounded ?p))"],
      "effects": [
        "(wounded ?p)",
        "(trap-triggered ?trap ?cave)"
      ]
    },
    {
      "name": "retreat-to-jungle-edge",
      "parameters": ["?p - player", "?cave - location", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?cave)", "(at-collapsed-cave)"],
      "effects": [
        "(not (player-at ?p ?cave))",
        "(player-at ?p ?jungle-edge)",
        "(not (at-collapsed-cave))",
        "(at-jungle-edge)"
      ]
    },
    {
      "name": "enter-cave-with-ally",
      "parameters": ["?p - player", "?native - ally", "?cave - location"],
      "preconditions": ["(player-at ?p ?cave)", "(ally-present ?native ?cave)", "(at-collapsed-cave)", "(not (medallion-found))"],
      "effects": []
    },
    {
      "name": "take-medallion-and-go-to-village-with-ally",
      "parameters": ["?p - player", "?native - ally", "?cave - location", "?village - location", "?medallion - item"],
      "preconditions": ["(player-at ?p ?cave)", "(ally-present ?native ?cave)", "(at-collapsed-cave)", "(not (medallion-found))"],
      "effects": [
        "(medallion-found)",
        "(has ?p ?medallion)",
        "(not (player-at ?p ?cave))",
        "(player-at ?p ?village)",
        "(ally-present ?native ?village)",
        "(not (at-collapsed-cave))",
        "(at-village)",
        "(native-trust-earned)"
      ]
    },
    {
      "name": "explore-jungle-outskirts-with-ally",
      "parameters": ["?p - player", "?native - ally", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(ally-present ?native ?jungle-edge)", "(at-jungle-edge)", "(not (wounded ?p))"],
      "effects": []
    },
    {
      "name": "gain-ally-trust-with-gift",
      "parameters": ["?p - player", "?native - ally", "?shore - location"],
      "preconditions": ["(player-at ?p ?shore)", "(at-shore)", "(not (ally-present ?native ?shore))"],
      "effects": [
        "(ally-present ?native ?shore)"
      ]
    },
    {
      "name": "insist-on-alone",
      "parameters": ["?p - player", "?native - ally", "?shore - location", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?shore)", "(at-shore)", "(not (ally-present ?native ?shore))"],
      "effects": [
        "(not (ally-present ?native ?shore))",
        "(not (player-at ?p ?shore))",
        "(player-at ?p ?jungle-edge)",
        "(not (at-shore))",
        "(at-jungle-edge)"
      ]
    },
    {
      "name": "ask-native-for-guidance",
      "parameters": ["?p - player", "?native - ally", "?shore - location", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?shore)", "(ally-present ?native ?shore)", "(at-shore)"],
      "effects": [
        "(not (player-at ?p ?shore))",
        "(player-at ?p ?jungle-edge)",
        "(not (at-shore))",
        "(at-jungle-edge)"
      ]
    },
    {
      "name": "visit-village-to-earn-trust",
      "parameters": ["?p - player", "?village - location"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(not (native-trust-earned))", "(medallion-found)"],
      "effects": [
        "(native-trust-earned)"
      ]
    },
    {
      "name": "attempt-negotiate-without-medallion",
      "parameters": ["?p - player", "?village - location"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(not (medallion-found))"],
      "effects": [
        "(wounded ?p)",
        "(fatally-injured ?p)"
      ]
    },
    {
      "name": "leave-village-to-find-medallion",
      "parameters": ["?p - player", "?village - location", "?cave - location"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(not (medallion-found))"],
      "effects": [
        "(not (player-at ?p ?village))",
        "(player-at ?p ?cave)",
        "(not (at-village))",
        "(at-collapsed-cave)"
      ]
    },
    {
      "name": "rest-at-jungle-edge",
      "parameters": ["?p - player", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)", "(wounded ?p)"],
      "effects": [
        "(not (wounded ?p))"
      ]
    },
    {
      "name": "travel-to-village-with-medallion",
      "parameters": ["?p - player", "?village - location", "?medallion - item"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(at-jungle-edge)", "(medallion-found)", "(has ?p ?medallion)"],
      "effects": [
        "(not (player-at ?p ?jungle-edge))",
        "(player-at ?p ?village)",
        "(not (at-jungle-edge))",
        "(at-village)"
      ]
    },
    {
      "name": "explore-jungle-outskirts-with-medallion",
      "parameters": ["?p - player", "?native - ally", "?jungle-edge - location", "?medallion - item"],
      "preconditions": ["(player-at ?p ?jungle-edge)", "(ally-present ?native ?jungle-edge)", "(at-jungle-edge)", "(medallion-found)", "(has ?p ?medallion)"],
      "effects": []
    },
    {
      "name": "earn-native-trust-at-village",
      "parameters": ["?p - player", "?village - location"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(medallion-found)", "(not (native-trust-earned))"],
      "effects": [
        "(native-trust-earned)"
      ]
    },
    {
      "name": "journey-to-temple-entrance",
      "parameters": ["?p - player", "?village - location", "?temple - location"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(native-trust-earned)"],
      "effects": [
        "(not (player-at ?p ?village))",
        "(player-at ?p ?temple)",
        "(not (at-village))",
        "(temple-accessible)"
      ]
    },
    {
      "name": "learn-altar-puzzle",
      "parameters": ["?p - player", "?village - location", "?puzzle - puzzle"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(native-trust-earned)"],
      "effects": [
        "(puzzle-solved)"
      ]
    },
    {
      "name": "rest-before-final-leg",
      "parameters": ["?p - player", "?village - location"],
      "preconditions": ["(player-at ?p ?village)", "(at-village)", "(not (wounded ?p))"],
      "effects": []
    },
    {
      "name": "attempt-solve-altar-puzzle",
      "parameters": ["?p - player", "?temple - location", "?puzzle - puzzle"],
      "preconditions": ["(player-at ?p ?temple)", "(temple-accessible)"],
      "effects": [
        "(puzzle-solved)"
      ]
    },
    {
      "name": "search-temple-for-hints",
      "parameters": ["?p - player", "?temple - location"],
      "preconditions": ["(player-at ?p ?temple)", "(temple-accessible)", "(not (wounded ?p))"],
      "effects": []
    },
    {
      "name": "enter-treasure-vault",
      "parameters": ["?p - player", "?temple - location"],
      "preconditions": ["(player-at ?p ?temple)", "(temple-accessible)", "(puzzle-solved)"],
      "effects": [
        "(treasure-claimed)"
      ]
    },
    {
      "name": "inspect-treasure-for-traps",
      "parameters": ["?p - player", "?temple - location"],
      "preconditions": ["(player-at ?p ?temple)", "(temple-accessible)", "(not (wounded ?p))"],
      "effects": []
    },
    {
      "name": "take-treasure-and-leave",
      "parameters": ["?p - player", "?temple - location"],
      "preconditions": ["(player-at ?p ?temple)", "(temple-accessible)", "(puzzle-solved)", "(treasure-claimed)"],
      "effects": []
    },
    {
      "name": "push-forward-to-find-medallion-wounded",
      "parameters": ["?p - player", "?cave - location", "?medallion - item"],
      "preconditions": ["(player-at ?p ?cave)", "(at-collapsed-cave)", "(wounded ?p)"],
      "effects": [
        "(medallion-found)",
        "(has ?p ?medallion)"
      ]
    },
    {
      "name": "retreat-to-jungle-edge-to-heal",
      "parameters": ["?p - player", "?cave - location", "?jungle-edge - location"],
      "preconditions": ["(player-at ?p ?cave)", "(at-collapsed-cave)", "(wounded ?p)"],
      "effects": [
        "(not (wounded ?p))",
        "(not (player-at ?p ?cave))",
        "(player-at ?p ?jungle-edge)",
        "(not (at-collapsed-cave))",
        "(at-jungle-edge)"
      ]
    },
    {
      "name": "attempt-call-for-help-while-wounded",
      "parameters": ["?p - player", "?native - ally", "?cave - location"],
      "preconditions": ["(player-at ?p ?cave)", "(at-collapsed-cave)", "(wounded ?p)", "(not (ally-present ?native ?cave))"],
      "effects": [
        "(fatally-injured ?p)"
      ]
    }
  ]
}